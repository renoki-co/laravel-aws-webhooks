{
    "name": "renoki-co/laravel-aws-webhooks",
    "description": "Easy webhook handler for Laravel to catch AWS SNS notifications for various services.",
    "keywords": [
        "laravel",
        "php",
        "sns",
        "aws",
        "webhooks",
        "ses",
        "webhook"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-aws-webhooks",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/http": "^8.83|^9.0.1",
        "illuminate/support": "^8.83|^9.0.1",
        "rennokki/laravel-sns-events": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\AwsWebhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\AwsWebhooks\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.28|^7.0",
        "orchestra/testbench": "^6.28|^7.0",
        "orchestra/testbench-core": "^6.x-dev",
        "phpunit/phpunit": "^9.5.21"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\AwsWebhooks\\AwsWebhooksServiceProvider"
            ]
        }
    }
}
